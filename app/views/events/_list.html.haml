.table-responsive
  %table.table.table-condensed.table-striped.table-vcentered
    %thead
      %tr
        %th name
        %th start
        %th end
        %th price

    %tbody
    
      - events.each do |event|
        %tr
         
          %td= event.name
          
          %td= event.start_time.strftime("%d.%m.%Y %H:%M  %A")
          %td= event.end_time.strftime("%d.%m.%Y %H:%M  %A")
          %td= event.price
          
            
    